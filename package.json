  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arles96/typescript-express/issues"
  },
  "homepage": "https://github.com/Arles96/typescript-express#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
