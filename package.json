{
  "name": "typescript-express",
  "version": "1.0.0",
  "description": "Estructura de typescript con express",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc && npm run public",
    "public": "cp -r ./src/public ./build",
